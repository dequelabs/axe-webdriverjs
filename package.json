{
  "name": "axe-webdriverjs",
  "description": "Provides a method to inject and analyze web pages using aXe",
  "version": "2.3.0",
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "contributors": [
    {
      "name": "Dylan Barrell",
      "organization": "Deque Systems, Inc.",
      "url": "http://github.com/dylanb/"
    },
    {
      "name": "Marcy Sutton",
      "organization": "Deque Systems, Inc.",
      "url": "http://github.com/marcysutton/"
    },
    {
      "name": "Wilco Fiers",
      "organization": "Deque Systems, Inc.",
      "url": "http://github.com/wilcofiers/"
    },
    {
      "name": "David Sturley",
      "organization": "Deque Systems, Inc.",
      "url": "http://deque.com/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dequelabs/axe-webdriverjs.git"
  },
  "keywords": [
    "a11y",
    "unit",
    "testing",
    "tdd",
    "bdd",
    "accessibility",
    "aXe",
    "selenium",
    "webdriver",
    "webdriverjs"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint './lib/**/*.js' './test/**/*.js'",
    "fmt": "prettier --write './lib/**/*.js' './test/**/*.js'",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha test/unit/*.js --require babel-register",
    "test:integration": "mocha test/integration/*.js --require babel-register",
    "test:sauce": "mocha test/sauce/*.js --require babel-register",
    "coverage": "nyc npm run test:unit",
    "build": "babel lib --out-dir dist",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "selenium-webdriver": ">= 2.53.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chromedriver": "^76.0.1",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.0.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.5",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.1",
    "selenium-webdriver": "^3.6.0",
    "sinon": "^7.3.2",
    "standard-version": "^7.0.0"
  },
  "dependencies": {
    "axe-core": "^3.3.2",
    "babel-runtime": "^6.26.0",
    "depd": "^2.0.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "bracketSpacing": true,
    "useTabs": false,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "overrides": [
      {
        "files": "test/**/*.js",
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
